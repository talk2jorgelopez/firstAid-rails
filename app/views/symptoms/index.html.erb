<%- model_class = Symptom -%>
<div class="page-header">
  <h3><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h3>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:symptom_type) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @symptoms.each do |symptom| %>
      <tr>
        <td><%= link_to symptom.id, symptom_path(symptom) %> | <%= symptoms_path( :id => symptom.id, :format => :json ) %></td>
        <td><%= symptom.symptom_type %> | <%= symptoms_path( :symptom_type => symptom.symptom_type, :format => :json ) %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_symptom_path(symptom), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      symptom_path(symptom),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<p>Json URL =

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_symptom_path,
            :class => 'btn btn-primary' %>
