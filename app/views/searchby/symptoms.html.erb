<div class="container-fluid">
	<div class="row">
		<div class="col-xs-6">
			<h3>Search by symptoms</h3>
		</div>
	</div>
	<div class="row">
		<div class="col-xs-6">
		<%= form_tag(searchby_symptoms_path, :method => "get", id: "search-form") do %>
			<%= text_field_tag :search, params[:search], placeholder: "Search Sympstoms" %>
			<%= submit_tag "Search" %>
		<% end %>
		</div>
	</div>
</div>