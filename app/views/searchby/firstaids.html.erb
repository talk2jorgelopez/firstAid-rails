<div class="container">
	<div class="row">
		<div class="col-xs-6">
			<h4>Search by First Aid Emergency</h4>
		</div>
	</div>

	<div class="row">
		<div class="col-xs-6">
			<%= form_tag(searchby_firstaids_path, :method => "get", id: "searchby-form") do %>
				<%= text_field_tag :search, params[:search], placeholder: "Search First Aid Emergency" %>
				<%= submit_tag "Search" %>
			<% end %>
		</div>
	</div>
</div>