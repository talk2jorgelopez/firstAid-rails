<div class="container">
	<div class="row">
		<div class="col-xs-6">
			<h3>Search by Vitals</h3>
		</div>
	</div>

	<div class="row">
		<div class="col-xs-6">
			<%= form_tag(searchby_vitals_path, :method => "get", id: "searchby-form") do %>
				<%= text_field_tag :search, params[:search], placeholder: "Search Vitals" %>
				<%= submit_tag "Search" %>
			<% end %>
		</div>
	</div>
</div>