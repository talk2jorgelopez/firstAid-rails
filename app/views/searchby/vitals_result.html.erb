<%- model_class = Vital -%>
<div class="page-header">
	<h3><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h3>
</div>
<table class="table table-striped">
	<thead>
	<tr>
		<th><%= model_class.human_attribute_name(:vital_type) %></th>
	</tr>
	</thead>
	<tbody>
	<% @vitals.each do |vital| %>
		<tr>
			<td><%= vital.vital_type %></td>
		</tr>
	<% end %>
	</tbody>
</table>
